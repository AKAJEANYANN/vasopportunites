{
  "name": "selfCare",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "selfCareName": {
      "type": "string",
      "default": ""
    },
    "selfCarePhone": {
      "type": "string",
      "default": ""
    },
    "selfCareDateCreation": {
      "type": "date",
      "defaultFn": "now"
    },
    "selfCareApprouve": {
      "type": "boolean",
      "default": true
    },
    "selfCareDateApprouve": {
      "type": "date",
      "default": ""
    },
    "selfCareCode": {
      "type": "string",
      "default": ""
    },
    "selfCareActif": {
      "type": "boolean",
      "default": true
    },
    "selfCareDesactifDate": {
      "type": "date",
      "default": ""
    },
    "device_fcm_token": {
      "type": "string",
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "appAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "operator": {
      "type": "belongsTo",
      "model": "operator",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "number"
    }
  ],
  "methods": {}
}

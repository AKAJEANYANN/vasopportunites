{
  "name": "customerCare",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "customerCareName": {
      "type": "string",
      "default": ""
    },
    "customerCarePhone": {
      "type": "string",
      "default": ""
    },
    "customerCareDateCreation": {
      "type": "date",
      "defaultFn": "now"
    },
    "customerCareApprouve": {
      "type": "boolean",
      "default": true
    },
    "customerCareDateApprouve": {
      "type": "date",
      "default": ""
    },
    "customerCareCode": {
      "type": "string",
      "default": ""
    },
    "customerCareActif": {
      "type": "boolean",
      "default": true
    },
    "customerCareDesactifDate": {
      "type": "date",
      "default": ""
    },
    "device_fcm_token": {
      "type": "string",
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "appAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "number"
    }
  ],
  "methods": {}
}

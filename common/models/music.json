{
  "name": "music",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "musicName": {
      "type": "string",
      "default": ""
    },
    "musicEtat": {
      "type": "string",
      "default": "EN_ATTENTE"
    },
    "musicActionDate": {
      "type": "date"
    },
    "musicDateCreated": {
      "type": "date",
      "defaultFn": "now"
    },
    "musicSupprime": {
      "type": "boolean",
      "default": false
    },
    "musicDateSupprime": {
      "type": "date"
    },
    "musicAudioUrl": {
      "type": "string",
      "default": ""
    },
    "operatorId": {
      "type": "string",
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "message": {
      "type": "belongsTo",
      "model": "message",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "selfCare": {
      "type": "belongsTo",
      "model": "selfCare",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "adminCare": {
      "type": "belongsTo",
      "model": "adminCare",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "countMusicByStatus"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "valideMusic"
    }
  ],
  "methods": {}
}
